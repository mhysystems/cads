  "version": "0.2.0",
  "configurations": [
    {
      "name": "encode",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/bin/Debug/net7.0/keygen.dll",
      "args": ["encode","--key", "abc", "--serial", "1234"],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    },
    {
      "name": "decode",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/bin/Debug/net7.0/keygen.dll",
      "args": ["decode","--key", "abc", "--serial", "uJyJsPSspvSjMjnG5+62XQ==!p8q3gWA6cZL7EVIa3DFCdw=="],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}